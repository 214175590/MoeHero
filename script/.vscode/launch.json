{
    "version": "0.0.1",
    "debugServer" : 4278,
    "configurations": [
        {
            "name": "attach",
            "type": "lua",
            "request": "attach",
			"program": "${workspaceRoot}/main.lua",
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "launch",
            "type": "lua",
            "request": "launch",
			"program": "${workspaceRoot}/main.lua",
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}",
			"path": "${workspaceRoot}/?.lua",
			"cpath": "${workspaceRoot}/?.dll"
        }
    ]
}
